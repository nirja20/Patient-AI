{
  "expo": {
    "name": "Patient Chatbot",
    "slug": "patient-chatbot-mobile",
    "scheme": "patientchatbot",
    "version": "1.0.0",
    "orientation": "default",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.futurrizon.patientchatbot",
      "usesCleartextTraffic": true,
      "permissions": [
        "INTERNET"
      ],
      "softwareKeyboardLayoutMode": "resize"
    },
    "plugins": [
      "expo-asset",
      "expo-web-browser",
      "@react-native-community/datetimepicker",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7fb814a4-9624-4e04-a55c-92a14c6356b1"
      }
    }
  }
}
