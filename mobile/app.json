{
  "expo": {
    "name": "Patient Chatbot",
    "slug": "patient-chatbot-mobile",
    "scheme": "patientchatbot",
    "version": "1.0.0",
    "orientation": "default",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.futurrizon.patientchatbot",
      "softwareKeyboardLayoutMode": "resize"
    },
    "plugins": [
      "expo-asset",
      "expo-web-browser",
      "@react-native-community/datetimepicker"
    ]
  }
}
